    "keywords": ["serialize", "serialization", "unserialize"],
    "homepage": "https://github.com/cs278/serialization-helpers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Smith",
            "email": "chris@cs278.org",
            "homepage": "http://www.cs278.org/"
        }
    ],
    "support": {
